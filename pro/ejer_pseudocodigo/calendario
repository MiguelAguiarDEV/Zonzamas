
Funcion subCadena(posibleFecha, numeroMenor,numeroMayor = Nulo )
    Cadena posibleFecha
    Entero contador = numeroMenor
    si numeroMayor es nulo entonces
        numeroMayor = posibleFecha.tamanho -1

    fin si

    Mientras contador < numeroMayor hacer
        contador++;

    devolver contador

Fin funcion


Funcion esFecha(Cadena posibleFecha)
        Logica FechaValida = Verdadero;
        Si posibleFecha.tamanho != 8 entonces
            fechaValida = Falso
        sino 
                si isNumeric(posibleFecha[subCadena(posibleFecha, 0, posibleFecha.tamanho-1)])
                    fechaValida = Verdadero;
                Fin si
        Fin si
        devolver fechaValida;

Funcion esMarzo2021(posibleFecha) 
        






si esMarzo2021(posibleFecha) entonces
    Escribir("Fecha dentro de marzo de 2021");
si no
    si esFecha(posibleFecha) entonces
        Escribir("Es una fecha cualquiera");
    si no
        Escribir("No es una fecha");
    fin si
fin si